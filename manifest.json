{
    "manifest_version": 2,
    "name": "Google Main Page",
    "version": "1.0",
    "description": "Replace Edge Main Home Page With Google",
    "permissions": ["activeTab"],
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "icons": {
      "48": "icon.png"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  